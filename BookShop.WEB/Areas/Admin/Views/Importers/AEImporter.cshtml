@model BookShop.WEB.DataBase.Entities.Importer

<div class="page-heading contact-heading header-text">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="text-content">
                    <h4>Редактирование таблицы</h4>
                    <h2>Импортёры</h2>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="container">
    <div class="edditForm">
        <form asp-controller="Importers" asp-action="AEImporter" method="post">
            <input asp-for="FullNameImporter" type="text" placeholder="Введите полное наименование (Пример: ОАО <<Капуста>>)" />
            <span style="color:red" asp-validation-for="FullNameImporter"></span>
            <input asp-for="AddressImpoter" type="text" placeholder="Введите адрес (Пример: РФ, г.Москва, ул. Личная 12)" />
            <span style="color:red" asp-validation-for="AddressImpoter"></span>
            <button type="submit" id="form-submit" class="filled-button">Принять</button>
        </form>
    </div>
</div>